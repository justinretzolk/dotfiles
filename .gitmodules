[submodule "vim-plug"]
  path  = vim/.vim/autoload
  url   = https://github.com/junegunn/vim-plug.git

[submodule "oh-my-zsh"]
  path  = zsh/.oh-my-zsh
  url   = https://github.com/robbyrussell/oh-my-zsh.git

[submodule "spaceship-prompt"]
  path  = zsh/custom/themes/spaceship-prompt
